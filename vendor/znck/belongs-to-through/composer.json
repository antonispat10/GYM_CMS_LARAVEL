    "satooshi/php-coveralls": "^1.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/znck/belongs-to-through",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "autoload": {
